{
    "name": "open-related-website",
    "description": "A Chrome extension that enables you to effortlessly open websites related to the current page you're browsing.",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "eslintConfig": {
        "extends": "@yutengjing/eslint-config-vue",
        "settings": {
            "import/resolver": {
                "typescript": {}
            }
        }
    },
    "prettier": "@yutengjing/prettier-config",
    "scripts": {
        "postinstall": "wxt prepare",
        "dev": "wxt",
        "dev:firefox": "wxt -b firefox",
        "build": "wxt build",
        "build:firefox": "wxt build -b firefox",
        "zip": "wxt zip",
        "zip:firefox": "wxt zip -b firefox",
        "compile": "vue-tsc --noEmit",
        "format": "prettier --write .",
        "lint": "eslint ."
    },
    "dependencies": {
        "@vueuse/core": "^10.9.0",
        "fuse.js": "^7.0.0",
        "vue": "^3.4.21"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.4",
        "@yutengjing/eslint-config-vue": "^0.6.0",
        "@yutengjing/prettier-config": "^1.1.2",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "postcss": "^8.4.35",
        "postcss-load-config": "^5.0.3",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.12",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.2",
        "vue-tsc": "^2.0.6",
        "wxt": "^0.17.7"
    }
}
